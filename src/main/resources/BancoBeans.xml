<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	<bean id="Cliente" class="com.beeva.spring.BancoSpringBeans.entity.Cliente" scope="prototype"/>
	<bean id="Cuenta" class="com.beeva.spring.BancoSpringBeans.entity.Cuenta" scope="prototype"/>
	<bean id="Banco" class="com.beeva.spring.BancoSpringBeans.store.Banco" scope="prototype"/>
	<bean id="Factory" class="com.beeva.spring.BancoSpringBeans.factory.CuentaFactory" scope="prototype"/>

	
	
</beans>